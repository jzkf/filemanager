{
    "name": "jzkf/filemanager",
    "description": "A comprehensive file management module for Yii2 framework with support for multiple storage drivers",
    "keywords": [
        "yii2",
        "file-manager",
        "file-upload",
        "storage",
        "flysystem",
        "s3",
        "oss",
        "cos"
    ],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/jzkf/filemanager/issues",
        "source": "https://github.com/jzkf/filemanager"
    },
    "authors": [
        {
            "name": "Your Name",
            "email": "your.email@example.com"
        }
    ],
    "require": {
        "php": ">=8.1.0",
        "ext-json": "*",
        "ext-gd": "*",
        "ext-fileinfo": "*",
        "yiisoft/yii2": "^2.0.45",
        "yiisoft/yii2-imagine": "*",
        "yiisoft/yii2-jui": "^2.0.4",
        "yiisoft/yii2-bootstrap5": "^2.0.4",
        "ramsey/uuid": "^4.2",
        "league/flysystem": "^3.0",
        "league/flysystem-local": "^3.0",
        "league/flysystem-aws-s3-v3": "^3.0",
        "league/mime-type-detection": "^1.0",
        "aws/aws-sdk-php": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.0",
        "codeception/codeception": "^5.0.0 || ^4.0"
    },
    "autoload": {
        "psr-4": {
            "jzkf\\filemanager\\": ""
        }
    },
    "extra": {
        "bootstrap": "jzkf\\filemanager\\FileManagerModule",
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    },
    "config": {
        "process-timeout": 1800
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}

